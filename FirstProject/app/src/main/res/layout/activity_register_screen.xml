        app:layout_constraintTop_toBottomOf="@+id/divider6" />

    <View
        android:id="@+id/divider8"
        android:layout_width="220dp"
        android:layout_height="1dp"
        android:layout_marginTop="35dp"
        android:background="?android:attr/listDivider"
